{
  "name": "is-figma-url",
  "version": "1.0.2",
  "description": "Simple regex to check whether a string is a valid Figma live embed url",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "figma",
    "figma-link",
    "figma-url",
    "figma-embed",
    "figma-live-embed"
  ],
  "author": "Florian Nagel <hi@floriannagel.xyz> (https://floriannagel.xyz/)",
  "engines": {
    "node": ">=20"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nagelflorian/is-figma-url.git"
  },
  "bugs": {
    "url": "https://github.com/nagelflorian/is-figma-url/issues"
  },
  "homepage": "https://github.com/nagelflorian/is-figma-url#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jest": "^30.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  }
}
